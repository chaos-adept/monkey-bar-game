{
  "name": "monkey-tournament",
  "version": "1.0.0",
  "description": "monkey tournament game",
  "main": "index.js",
  "scripts": {
    "start-server:debug": "cross-env NODE_ENV=development babel-node ./lib/index.js",
    "start-server": "cross-env NODE_ENV=production node ./build/server/index.js",
    "start-server:refresh": "npm run clean && npm run build && npm run start-server",
    "clean": "rimraf build",
    "build": "npm run build:server && npm run build:webpack",
    "build:server": "babel ./lib --out-dir build/server --require babel-polyfill --ignore ./lib/client,__tests__",
    "build:webpack": "cross-env NODE_ENV=production webpack --config ./lib/webpack/webpack.config.prod.js --bail",
    "etc:version": "node etc/copy-version.js",
    "test": "babel-node ./node_modules/mocha/bin/_mocha ./lib/**/*--specs.js",
    "test:watch": "babel-node ./node_modules/mocha/bin/_mocha --require babel-polyfill --watch ./lib/**/*--specs.js",
    "test:integration": "babel-node ./node_modules/mocha/bin/_mocha --require babel-polyfill ./lib/__tests__/**/*--integration-specs.js",
    "test:integration-debug": "babel-node ./node_modules/mocha/bin/_mocha --require babel-polyfill --watch ./lib/__tests__/**/*--integration-specs.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chaos-adept/monkey-tournament.git"
  },
  "keywords": [
    "game"
  ],
  "author": "denis.rykovanov@mail.ru",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/chaos-adept/monkey-tournament/issues"
  },
  "homepage": "https://github.com/chaos-adept/monkey-tournament#readme",
  "devDependencies": {
    "babel-core": "6.11.4",
    "babel-eslint": "6.1.2",
    "babel-loader": "6.2.7",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-syntax-async-functions": "6.13.0",
    "babel-plugin-syntax-object-rest-spread": "6.13.0",
    "babel-plugin-transform-es2015-spread": "6.8.0",
    "babel-plugin-transform-object-assign": "6.8.0",
    "babel-plugin-transform-object-rest-spread": "6.16.0",
    "babel-polyfill": "6.16.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "6.16.0",
    "babel-register": "6.9.0",
    "chai": "3.5.0",
    "chai-datetime": "1.4.1",
    "chai-fuzzy": "1.6.1",
    "cross-env": "2.0.0",
    "css": "2.2.1",
    "css-loader": "0.23.1",
    "fast-async": "6.0.22",
    "git-describe": "3.0.2",
    "install": "0.8.1",
    "lodash": "4.15.0",
    "mocha": "2.5.3",
    "moment": "2.14.1",
    "node-sass": "3.8.0",
    "optimist": "0.6.1",
    "react": "15.3.1",
    "react-dom": "15.3.1",
    "react-router": "2.7.0",
    "rimraf": "2.5.4",
    "sass": "0.5.0",
    "sass-loader": "4.0.0",
    "selenium-webdriver": "2.53.3",
    "serve-static": "1.11.1",
    "socket.io-client": "1.4.8",
    "style": "0.0.3",
    "style-loader": "0.13.1",
    "superagent": "2.2.0",
