    "connect-ensure-login": "0.1.1",
    "connect-redis": "3.1.0",
    "cookie-parser": "1.4.3",
    "express": "4.14.0",
    "express-session": "1.14.0",
    "lodash": "4.13.1",
    "log4js": "0.6.38",
    "node-redis": "0.1.7",
    "passport": "0.3.2",
    "passport-local": "1.0.0",
    "passport-vkontakte": "0.3.1",
    "passport.socketio": "3.6.2",
    "promisify": "0.0.3",
