{
  "name": "monkey-tournament",
  "version": "1.0.0",
  "description": "monkey tournament game",
  "main": "index.js",
  "scripts": {
    "start-server:debug": "cross-env NODE_ENV=development babel-node ./lib/index.js",
    "start-server": "node ./build/index.js",
    "start-server:refresh": "npm run clean && npm run build && npm run start-server",
    "clean": "rimraf build",
    "build": "babel ./lib --out-dir build",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.js --bail",
    "etc:version": "node etc/copy-version.js",
    "test": "mocha --compilers js:babel-register --require babel-polyfill ./lib/**/*--specs.js",
    "test:watch": "mocha --compilers js:babel-register --require babel-polyfill --watch ./lib/**/*--specs.js",
    "test:integration": "mocha --compilers js:babel-register --require babel-polyfill ./lib/__tests__/**/*--integration-specs.js",
    "test:integration-debug": "mocha --compilers js:babel-register --require babel-polyfill --watch ./lib/__tests__/**/*--integration-specs.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chaos-adept/monkey-tournament.git"
  },
  "keywords": [
    "game"
  ],
  "author": "denis.rykovanov@mail.ru",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/chaos-adept/monkey-tournament/issues"
  },
  "homepage": "https://github.com/chaos-adept/monkey-tournament#readme",
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-syntax-async-functions": "^6.8.0",
    "babel-plugin-syntax-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-es2015-spread": "^6.8.0",
    "babel-plugin-transform-object-assign": "^6.5.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "chai-datetime": "^1.4.1",
    "chai-fuzzy": "^1.6.1",
    "cross-env": "^2.0.0",
    "fast-async": "^6.0.22",
    "git-describe": "^3.0.2",
    "install": "^0.8.1",
    "lodash": "^4.15.0",
    "mocha": "^2.5.3",
    "moment": "^2.14.1",
    "optimist": "^0.6.1",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-router": "^2.7.0",
    "rimraf": "^2.5.4",
    "selenium-webdriver": "^2.53.3",
    "superagent": "^2.1.0",
    "to-clipboard": "^0.2.0",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.2"
  },
  "dependencies": {
    "babel-cli": "^6.11.4",
    "babel-polyfill": "^6.9.1",
    "bluebird": "^3.4.1",
    "body-parser": "^1.15.2",
    "connect-ensure-login": "^0.1.1",
    "connect-redis": "^3.1.0",
    "cookie-parser": "^1.4.3",
    "express": "^4.14.0",
    "express-session": "^1.14.0",
    "lodash": "^4.13.1",
    "log4js": "^0.6.38",
    "node-redis": "^0.1.7",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "passport-vkontakte": "^0.3.1",
    "passport.socketio": "^3.6.2",
    "promisify": "0.0.3",
    "redis": "^2.6.2",
    "sinon": "^1.17.4",
    "socket.io": "^1.4.8",
    "socket.io-client": "^1.4.8"
  }
}
